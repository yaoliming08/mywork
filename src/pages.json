        "navigationBarTitleText": "",
        "disableScroll": true
      }
    },
    {
      "path": "pages/login/index",
      "lazyCodeLoading": "requiredComponents",
      "style": {
        "navigationBarTitleText": "",
        "disableScroll": true
      }
      
    },
    {
      "path": "pages/report-page/index",
      "lazyCodeLoading": "requiredComponents",
      "style": {
        "navigationBarTitleText": "",
        "disableScroll": true
      }
      
    }
